<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="LocationUpdate">
		<xs:complexType mixed="false">
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:element name="UpdateLocation" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:all>
							<!-- LocationID is deprecated as of Version 5.8b01, RetailerLocationCode should be now used -->
							<xs:element name="LocationID" type="twentyChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="RetailerLocationCode" type="twentyChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="LocationName" type="hundredChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Address1" type="twohundredChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Address2" type="twohundredChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="City" type="hundredChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="State" type="fiftyChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Zip" type="twentyChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="ISO3166Alpha2CountryCode" type="twoChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="TestingLocation" type="bit" minOccurs="0" maxOccurs="1"/>
							<xs:element name="BannerID" type="xs:nonNegativeInteger" minOccurs="0" maxOccurs="1"/>
							<xs:element name="TimeZone" type="twentyChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="ISO4217CurrencyCode" type="threeChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Contact" type="hundredChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="Phone" type="twentyChars" minOccurs="0" maxOccurs="1"/>
