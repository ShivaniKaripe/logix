							<xs:element name="UnitOfMeasureSetID" type="xs:int" minOccurs="0" maxOccurs="1" />
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="RemoveLocation" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:all>
							<!-- LocationID is deprecated as of Version 5.8b01, RetailerLocationCode should be now used -->
							<xs:element name="LocationID" type="twentyChars" minOccurs="0" maxOccurs="1"/>
							<xs:element name="RetailerLocationCode" type="twentyChars" minOccurs="0" maxOccurs="1"/>
						</xs:all>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
